.content-head__complete-button
	float right
	margin-top 7px
	margin-right 20px
	width 190px
	height 45px
	cursor pointer
	border-radius 30px
	border 2px solid green-text
	&:hover
		border 2px solid #4A8D64
.content-head__complete-button_title
	padding 7px
	text-align center
	sans-regular()
	font-size 16px
	color grey-text
.content-head__settings-button
	float right
	margin-top 7px
	margin-right 20px
	width 45px
	height 45px
	cursor pointer
	border-radius 30px
	border 2px solid #E0E2E3
	&:hover
		border 2px solid light-grey
.content-head__settings-button_icon
	font-awesome()
	padding 12px
	font-size 18px
	position absolute
	text-align center
	color #E0E2E3
	&:hover
		color light-grey
@media screen and (max-width 1080px)
	.content-head__complete-button
		display none
